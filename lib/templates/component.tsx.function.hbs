{{#if css}}
  import styles from "./{{name}}.module.css"
{{else}}
  import styles from "./{{name}}.module.scss";
{{/if}}

import React, { FC } from 'react';

{{#if test}} export {{/if}} interface IProps {
  content: string;
}
/**
* {{name}}
*/
const {{ name }}: FC<IProps> = ({ content }) => (
  {{#if css}}
    <p className={styles.element}>{content}</p>
  {{else}}
    <p className={styles.element}>{content}</p>
  {{/if}}
)

export default {{name}};
